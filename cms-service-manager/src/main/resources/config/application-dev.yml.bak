server:
  port: 20000


spring:
  # MongoDB
  data:
    mongodb:
      uri: mongodb://127.0.0.1:27017
      database: cms

  # Freemarker
  freemarker:
    cache: false
    settings:
      template_update_delay: 0

  # RabbitMQ
  rabbitmq:
    host: localhost
    port: 5672
    username:
    password:
    virtual-host: /
    params:
      exchange:
        page-release: exchange_routing_cms_page_release